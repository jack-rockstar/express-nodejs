<%- include("../template/cabeza.ejs",{tituloWeb:'Pagina de inicio'});  %> 
      <h1><%= listaMascotas %> </h1>
    <div class="container">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Nombre</th>
                <th scope="col">Descripcion</th>
                <th scope="col">Accion</th>
              </tr>
            </thead>
            <tbody>
          
       <% if (arrays.length>0) { %>
         <% arrays.forEach(mascotas => { %>
            
                <tr>
                  <th scope="row"><%= mascotas.id %> </th>
                  <td><%= mascotas.nombre %></td>
                  <td><%= mascotas.descripcion %></td>
                  <td><%= mascotas.nombre %></td>
                </tr>
              

             <% }) %>
       <% } %>
       </tbody>
      </table>
    </div>
      
